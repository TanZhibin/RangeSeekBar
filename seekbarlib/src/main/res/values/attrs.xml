<?xml version="1.0" encoding="utf-8"?>
<resources>



    <declare-styleable name="RangeSeekBar">
        <attr name="rsb_main_color" format="color" />
        <attr name="rsb_vice_color" format="color" />
        <attr name="rsb_stroke_width" format="dimension" />
        <attr name="rsb_max" format="integer" />
        <attr name="rsb_progress" format="integer" />
        <attr name="rsb_radius" format="integer" />
        <attr name="rsb_center_indicator" format="boolean" />
        <attr name="rsb_thumb_color" format="color" />
        <attr name="rsb_thumb" format="reference" />
        <attr name="rsb_bg_res" format="reference" />
        <attr name="rsb_mode">
            <enum name="singleBar" value="1" />
            <enum name="doubleBar" value="2" />
        </attr>
    </declare-styleable>
</resources>
